[puppet5]
name=Puppet 5 Repository el $releasever - $basearch
baseurl=http://yum.puppetlabs.com/puppet5/el/$releasever/$basearch
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppet5
enabled=1
gpgcheck=1

[puppet5-source]
name=Puppet 5 Repository el $releasever - Source
baseurl=http://yum.puppetlabs.com/puppet5-nightly/el/$releasever/SRPMS
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-puppet5-release
failovermethod=priority
enabled=0
gpgcheck=1